##### Inert mode #####

#This mode bypasses all i3wm keys in case of compatibility problems:
mode "I"
{
	bindsym $mod+Escape mode "default"
}

bindsym $mod+Escape mode "I"


##### Workspace mode #####

#This mode lets yuu change workspaces more easily
mode "W"
{
	# Define focus movements:
	bindsym a $ws $ws1, mode "default"
	bindsym s $ws $ws2, mode "default"
	bindsym d $ws $ws3, mode "default"
	bindsym f $ws $ws4, mode "default"
	bindsym g $ws $ws5, mode "default"
	bindsym h $ws $ws6, mode "default"
	bindsym j $ws $ws7, mode "default"
	bindsym k $ws $ws8, mode "default"
	bindsym l $ws $ws9, mode "default"

	bindsym q $ws $ws0, mode "default"
	bindsym w $ws $ws11, mode "default"
	bindsym e $ws $ws12, mode "default"
	bindsym r $ws $ws13, mode "default"
	bindsym t $ws $ws14, mode "default"

	bindsym y $ws $ws10, mode "default"
	bindsym x $ws $ws21, mode "default"
	bindsym c $ws $ws22, mode "default"
	bindsym v $ws $ws23, mode "default"
	bindsym b $ws $ws14, mode "default"

	# Define window movements:
	bindsym Shift+a $mct $ws1, mode "default"
	bindsym Shift+s $mct $ws2, mode "default"
	bindsym Shift+d $mct $ws3, mode "default"
	bindsym Shift+f $mct $ws4, mode "default"
	bindsym Shift+g $mct $ws5, mode "default"
	bindsym Shift+h $mct $ws6, mode "default"
	bindsym Shift+j $mct $ws7, mode "default"
	bindsym Shift+k $mct $ws8, mode "default"
	bindsym Shift+l $mct $ws9, mode "default"

	bindsym Shift+q $mct $ws0, mode "default"
	bindsym Shift+w $mct $ws11, mode "default"
	bindsym Shift+e $mct $ws12, mode "default"
	bindsym Shift+r $mct $ws13, mode "default"
	bindsym Shift+t $mct $ws14, mode "default"

	bindsym Shift+y $mct $ws10, mode "default"
	bindsym Shift+x $mct $ws21, mode "default"
	bindsym Shift+c $mct $ws22, mode "default"
	bindsym Shift+v $mct $ws23, mode "default"
	bindsym Shift+b $mct $ws14, mode "default"
	
	#regular focus movements:
	bindsym Control+h focus left
	bindsym Control+j focus down
	bindsym Control+k focus up
	bindsym Control+l focus right

	# Return keys:
	bindsym Escape mode "default"
	bindsym Return mode "default"	
}

bindsym $mod+0 mode "W"
bindsym $mod+odiaeresis mode "W"

